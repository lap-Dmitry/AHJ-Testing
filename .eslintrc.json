{
  "extends": "airbnb-base",
  "env": {
     "es6": true,
     "browser": true,
     "jest": true
  },
  "rules": {
    "no-param-reassign": 0,
    "no-console": "off",
    "no-restricted-syntax": [
      "error",
      "LabeledStatement",
      "WithStatement"
    ],    
    "linebreak-style": [
      "error",
      "unix"
  ],
  "import/no-extraneous-dependencies": [
    "error",
    { 
      "devDependencies": true 
    }
  ]
  }
}